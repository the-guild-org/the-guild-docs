{
  "name": "@guild-docs/server",
  "version": "4.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-guild-org/the-guild-docs.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./postcss.config": {
      "require": "./dist/postcss.config.cjs"
    },
    "./tailwind.config": {
      "require": "./dist/tailwind.config.cjs"
    },
    "./*": "./dist/*.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bob-esbuild watch",
    "build": "bob-esbuild build",
    "postpublish": "gh-release",
    "test:npm": "bob-tsm test-npm.ts"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.2.2",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.12",
    "mermaid": "^9.1.3",
    "nextra": "2.0.0-alpha.56",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "rehype-mdx-title": "^1.0.0",
    "tailwindcss": "^3.1.6",
    "tiny-lru": "^8.0.2",
    "undici": "5.5.1"
  },
  "peerDependencies": {
    "next": ">=12.1.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*",
        "dist/*/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@types/mermaid": "^8.2.9"
  }
}
