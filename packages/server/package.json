{
  "name": "@guild-docs/server",
  "version": "2.1.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-guild-org/the-guild-docs.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./*": {
      "require": "./lib/*.js",
      "import": "./lib/*.mjs"
    }
  },
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "bob-esbuild watch",
    "prepack": "bob-esbuild build",
    "postpublish": "gh-release",
    "test:npm": "bob-tsm test-npm.ts"
  },
  "dependencies": {
    "@guild-docs/mdx-remote": "workspace:^1.0.1",
    "@guild-docs/types": "workspace:^1.0.3",
    "github-slugger": "^1.4.0",
    "globby": "^11.0.4",
    "gray-matter": "^4.0.3",
    "node-fetch": "^2.6.6",
    "rehype-slug": "^5.0.0",
    "remark-emoji": "^3.0.2",
    "tiny-lru": "^7.0.6",
    "unified": "^10.1.1",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@types/github-slugger": "1.3.0",
    "@types/node-fetch": "^2.5.12",
    "bob-esbuild-cli": "^3.0.1",
    "changesets-github-release": "0.0.4",
    "esbuild": "^0.13.14",
    "next": "^12.0.4",
    "next-i18next": "^9.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark-admonitions": "1.2.1",
    "shiki": "^0.9.12",
    "undici": "^4.10.2"
  },
  "peerDependencies": {
    "next": ">=11.1.2",
    "next-i18next": "*",
    "react": "*",
    "react-dom": "*",
    "remark-admonitions": "*",
    "shiki": "*"
  },
  "peerDependenciesMeta": {
    "remark-admonitions": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  },
  "typesVersions": {
    "*": {
      "lib/index.d.ts": [
        "lib/index.d.ts"
      ],
      "*": [
        "lib/*",
        "lib/*/index.d.ts"
      ]
    }
  }
}
