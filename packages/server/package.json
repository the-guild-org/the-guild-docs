    "postpublish": "gh-release",
    "test:npm": "bob-tsm test-npm.ts"
  },
  "dependencies": {
    "@guild-docs/mdx-remote": "workspace:^1.0.2",
    "@guild-docs/types": "workspace:^1.1.0",
    "@stefanprobst/remark-shiki": "^2.1.0",
    "github-slugger": "^1.4.0",
    "globby": "^13.1.1",
    "gray-matter": "^4.0.3",
    "rehype-slug": "^5.0.1",
    "remark-emoji": "^3.0.2",
    "tiny-lru": "^8.0.1",
    "undici": "^4.14.1"
  },
  "devDependencies": {
    "@types/github-slugger": "1.3.0",
    "bob-esbuild-cli": "^3.0.1",
    "changesets-github-release": "0.1.0",
    "esbuild": "^0.14.23",
    "next": "^12.1.0",
    "next-i18next": "^10.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "shiki": "^0.10.0"
  },
  "peerDependencies": {
    "next": ">=12.0.10",
    "next-i18next": "*",
    "react": "*",
    "react-dom": "*",
    "shiki": "*"
  },
