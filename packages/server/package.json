    "remark-slug": "^6.1.0",
    "tiny-lru": "^7.0.6"
  },
  "devDependencies": {
    "@types/github-slugger": "1.3.0",
    "bob-esbuild-cli": "0.1.27",
    "changesets-github-release": "0.0.3",
    "esbuild": "0.12.15",
    "next": "^11.0.1",
    "next-i18next": "8.5.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
