{
  "name": "@guild-docs/server",
  "version": "2.2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-guild-org/the-guild-docs.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bob-esbuild watch",
    "prepack": "bob-esbuild build",
    "postpublish": "gh-release",
    "test:npm": "bob-tsm test-npm.ts"
  },
  "dependencies": {
    "@guild-docs/mdx-remote": "workspace:^1.0.2",
    "@guild-docs/types": "workspace:^1.1.0",
    "github-slugger": "^1.4.0",
    "globby": "^11.1.0",
    "gray-matter": "^4.0.3",
    "rehype-slug": "^5.0.1",
    "remark-emoji": "^3.0.2",
    "tiny-lru": "^8.0.1",
    "undici": "^4.14.1",
    "unified": "^10.1.1",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@types/github-slugger": "1.3.0",
    "bob-esbuild-cli": "^3.0.1",
    "changesets-github-release": "0.1.0",
    "esbuild": "^0.14.22",
    "next": "^12.1.0",
    "next-i18next": "^10.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark-admonitions": "1.2.1",
    "shiki": "^0.10.1"
  },
  "peerDependencies": {
    "next": ">=11.1.4",
    "next-i18next": "*",
    "react": "*",
    "react-dom": "*",
    "remark-admonitions": "*",
    "shiki": "*"
  },
  "peerDependenciesMeta": {
    "remark-admonitions": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*",
        "dist/*/index.d.ts"
      ]
    }
  }
}
