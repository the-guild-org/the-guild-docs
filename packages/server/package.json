{
  "name": "@guild-docs/server",
  "version": "3.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-guild-org/the-guild-docs.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bob-esbuild watch",
    "prepack": "bob-esbuild build",
    "postpublish": "gh-release",
    "test:npm": "bob-tsm test-npm.ts"
  },
  "dependencies": {
    "@guild-docs/mdx-remote": "workspace:^2.0.1",
    "@guild-docs/types": "workspace:^2.0.0",
    "@stefanprobst/remark-shiki": "2.1.0",
    "github-slugger": "1.4.0",
    "globby": "13.1.1",
    "gray-matter": "4.0.3",
    "rehype-slug": "5.0.1",
    "remark-emoji": "3.0.2",
    "tiny-lru": "8.0.2",
    "undici": "5.0.0"
  },
  "devDependencies": {
    "@types/github-slugger": "1.3.0",
    "bob-esbuild-cli": "3.0.2",
    "changesets-github-release": "0.1.0",
    "esbuild": "0.14.34",
    "next": "12.1.4",
    "next-i18next": "11.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "shiki": "0.10.1"
  },
  "peerDependencies": {
    "next": ">=12.1.4",
    "next-i18next": "*",
    "react": "*",
    "react-dom": "*",
    "shiki": "*"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*",
        "dist/*/index.d.ts"
      ]
    }
  }
}
