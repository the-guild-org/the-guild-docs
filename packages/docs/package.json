    "framer-motion": "6.5.1",
    "mermaid": "^9.1.3",
    "next-seo": "5.4.0",
    "nextra": "2.0.0-alpha.56",
    "nprogress": "0.2.0",
    "react-icons": "4.4.0",
    "react-use": "17.4.0",
