    "changesets-github-release": "^0.0.4",
    "commander": "^8.3.0",
    "edit-json-file": "^1.6.2",
    "esbuild": "^0.13.15",
    "mkdirp": "^1.0.4",
    "npm-api": "^1.0.1",
    "prettier": "^2.5.0"
  }
