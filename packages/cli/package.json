{
  "name": "guild-docs",
  "version": "0.2.15",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-guild-org/the-guild-docs.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "bin": {
    "guild-docs": "lib/index.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsup --silent",
    "dev": "tsup --watch --onSuccess \"pnpm dev-start\"",
    "dev-start": "node lib/index.js init ../../examples/basic && pnpm -w pretty",
    "prepack": "pnpm build",
    "postpublish": "gh-release",
    "test-new": "rimraf \"../../examples/new\" && tsup --silent && node lib/index.js init \"../../examples/new\""
  },
  "devDependencies": {
    "@types/edit-json-file": "1.4.0",
    "@types/mkdirp": "1.0.1",
    "@types/node": "16.0.0",
    "@types/prettier": "2.3.1",
    "changesets-github-release": "0.0.3",
    "commander": "8.0.0",
    "edit-json-file": "1.6.0",
    "esbuild": "0.12.14",
    "mkdirp": "1.0.4",
    "npm-api": "1.0.1",
    "prettier": "2.3.2",
    "tsup": "4.12.0"
  },
  "tsup": {
    "splitting": false,
    "outDir": "lib",
    "entryPoints": [
      "src/index.ts"
    ]
  }
}
