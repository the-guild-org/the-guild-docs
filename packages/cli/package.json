  "bin": {
    "guild-docs": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bob-tsm --cjs build.ts",
    "dev": "bob-watch --watch=src -c \"pnpm dev-start\"",
    "dev-start": "pnpm build && node dist/index.js init ../../examples/basic && pnpm -w pretty",
    "prepack": "pnpm build",
    "postpublish": "gh-release",
    "test-new": "rimraf \"../../examples/new\" && node dist/index.js init \"../../examples/new\""
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.8",
    "@types/prettier": "^2.4.3",
    "bob-tsm": "^0.4.4",
    "bob-watch": "^0.1.1",
    "changesets-github-release": "^0.0.4",
    "commander": "^8.3.0",
    "esbuild": "^0.14.11",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "npm-api": "^1.0.1",
    "prettier": "^2.5.1"
  }
}
